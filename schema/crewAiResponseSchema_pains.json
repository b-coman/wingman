{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "result": {
            "type": "object",
            "properties": {
                "pains": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "painSKU": {
                                "type": "string"
                            },
                            "reason": {
                                "type": "string"
                            },
                            "confidenceScore": {
                                "type": "string",
                                "pattern": "^(0(\\.\\d+)?|1(\\.0+)?)$"
                            }
                        },
                        "required": [
                            "painSKU",
                            "reason",
                            "confidenceScore"
                        ],
                        "additionalProperties": false
                    }
                }
            },
            "required": ["pains"],
            "additionalProperties": false
        },
        "status": {
            "type": "string"
        }
    },
    "required": [
        "result",
        "status"
    ],
    "additionalProperties": false
}
